{
  "name": "ubersetzerli",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "lint": "ng lint",
    "start": "ng serve --host 0.0.0.0",
    "build": "ng build --configuration=production --base-href=/",
    "build:stats": "ng build --stats-json",
    "analyze": "webpack-bundle-analyzer dist/ubersetzerli/browser/stats.json",
    "pwa:assets": "pwa-asset-generator src/assets/icons/dark-logo.svg src/assets/icons/generated/ -i src/index.html -m src/manifest.webmanifest --background white --favicon --mstile && pwa-asset-generator src/assets/icons/light-logo.svg --dark-mode src/assets/icons/generated/ -i src/index.html --background black --splash-only",
    "capacitor:assets": "capacitor-assets generate --verbose --ios --android --assetPath src/assets/icons --iconBackgroundColor=white --iconBackgroundColorDark=black --splashBackgroundColor=white --splashBackgroundColorDark=black --logoSplashTargetWidth=768",
    "test": "ng test --code-coverage",
    "test:chrome": "ng test --browsers=Chrome --code-coverage",
    "test:ci": "ng test --browsers=ChromeHeadless,FirefoxHeadless --watch=false --code-coverage",
    "deploy": "firebase deploy --only=hosting",
    "mobile:sync": "tsx tools/mobile/sync.ts",
    "mobile:metadata": "tsx tools/mobile/metadata/metadata.ts",
    "prepare:husky": "node -e \"require('fs').cpSync('node_modules/@sign-mt/configuration/.husky/', '.husky/', {recursive: true})\"",
    "prepare": "husky install && npm run prepare:husky",
    "dev:ssr": "ng run ubersetzerli:serve-ssr",
    "serve:ssr": "node dist/ubersetzerli/server/main.js",
    "build:ssr": "ng build && ng run ubersetzerli:server",
    "prerender": "ng run ubersetzerli:prerender:production --routes /"
  },
  "engines": {
    "node": "16",
    "npm": "8"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "16.1.8",
    "@angular/cdk": "16.1.7",
    "@angular/common": "16.1.8",
    "@angular/compiler": "16.1.8",
    "@angular/core": "16.1.8",
    "@angular/forms": "16.1.8",
    "@angular/material": "16.1.7",
    "@angular/platform-browser": "16.1.8",
    "@angular/platform-browser-dynamic": "16.1.8",
    "@angular/platform-server": "16.1.8",
    "@angular/router": "16.1.8",
    "@angular/service-worker": "16.1.8",
    "@asymmetrik/ngx-leaflet": "16.0.1",
    "@capacitor-firebase/analytics": "^5.1.0",
    "@capacitor-firebase/app": "^5.1.0",
    "@capacitor-firebase/crashlytics": "^5.1.0",
    "@capacitor-firebase/performance": "^5.1.0",
    "@capacitor/android": "^5.2.2",
    "@capacitor/core": "^5.2.2",
    "@capacitor/filesystem": "^5.1.1",
    "@capacitor/ios": "^5.2.2",
    "@capacitor/keyboard": "^5.0.6",
    "@capacitor/share": "^5.0.6",
    "@capacitor/splash-screen": "^5.0.6",
    "@ngneat/transloco": "^5.0.6",
    "@nguniversal/express-engine": "^16.1.2",
    "@ngxs/store": "3.8.1",
    "@sign-mt/browsermt": "^0.2.1",
    "@sign-mt/i18n": "git://github.com/sign/i18n.git",
    "capacitor-blob-writer": "^1.1.14",
    "cld3-asm": "^3.1.1",
    "comlink": "^4.4.1",
    "filesize": "^9.0.11",
    "firebase": "10.1.0",
    "flag-icons": "^6.9.3",
    "leaflet": "^1.9.4",
    "ngx-filesize": "^3.0.2",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.1",
    "web-vitals": "^3.4.0",
    "zone.js": "~0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1601.8",
    "@angular-devkit/build-angular": "^16.1.8",
    "@angular-eslint/builder": "^16.1.0",
    "@angular-eslint/eslint-plugin": "^16.1.0",
    "@angular-eslint/eslint-plugin-template": "^16.1.0",
    "@angular-eslint/schematics": "^16.1.0",
    "@angular-eslint/template-parser": "^16.1.0",
    "@angular/cli": "^16.1.8",
    "@angular/compiler-cli": "^16.1.8",
    "@capacitor/assets": "^2.0.4",
    "@capacitor/cli": "^5.2.2",
    "@nguniversal/builders": "^16.1.2",
    "@playwright/test": "^1.36.2",
    "@sign-mt/configuration": "git://github.com/sign/.github.git",
    "@trapezedev/project": "^7.0.10",
    "@types/dom-speech-recognition": "^0.0.1",
    "@types/jasmine": "~4.3.5",
    "@types/jasminewd2": "~2.0.10",
    "@types/web-app-manifest": "^1.0.4",
    "@typescript-eslint/eslint-plugin": "6.2.1",
    "@typescript-eslint/parser": "6.2.1",
    "deepmerge": "^4.3.1",
    "dotenv": "^16.3.1",
    "eslint": "^8.46.0",
    "fuzzy": "^0.1.3",
    "husky": "^8.0.3",
    "inquirer": "^9.2.10",
    "inquirer-autocomplete-prompt": "^3.0.0",
    "jasmine-axe": "^1.1.0",
    "jasmine-core": "~5.1.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "^6.4.2",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-firefox-launcher": "^2.1.2",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "karma-safari-launcher": "^1.0.0",
    "karma-spec-reporter": "^0.0.36",
    "lint-staged": "^13.2.3",
    "open": "^9.1.0",
    "pwa-asset-generator": "^6.3.1",
    "sitemap": "^7.1.1",
    "tiny-async-pool": "^2.1.0",
    "ts-node": "~10.9.1",
    "tsx": "^3.12.7",
    "typescript": "~4.9.3",
    "webpack-bundle-analyzer": "^4.9.0",
    "zod": "^3.21.4"
  }
}
